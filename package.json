  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SPFxAppDev/spfxcli/issues"
  },
  "homepage": "https://github.com/SPFxAppDev/spfxcli.git#readme",
  "dependencies": {
    "@azure/msal-node": "^5.0.4",
    "@azure/msal-node-extensions": "^5.0.4",
    "@pnp/graph": "^4.17.0",
    "@pnp/nodejs": "^4.17.0",
    "@pnp/sp": "^4.17.0",
    "@spfxappdev/utility": "^1.4.0",
    "@types/configstore": "^6.0.0",
    "chalk": "4.1.2",
    "cli-table3": "^0.6.3",
    "configstore": "5.0.1",
    "detect-indent": "6.1.0",
    "replace-in-file": "6.3.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "@types/yargs": "^17.0.24",
    "cpy-cli": "^5.0.0",
    "typescript": "^5.1.6"
  }
}